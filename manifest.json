{
  "name": "Web Visit Counter",
  "description": "Counter for web pages, that you've visited.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/rat16.png",
      "32": "/images/rat32.png",
      "48": "/images/rat48.png",
      "128": "/images/rat128.png"
    }
  },
  "icons": {
    "16": "/images/rat16.png",
    "32": "/images/rat32.png",
    "48": "/images/rat48.png",
    "128": "/images/rat128.png"
  },
  "options_page": "options.html"
}
